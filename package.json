    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "sass": "^1.47.0",
    "screenfull": "^6.0.1",
    "tinymce": "^5.10.2",
    "ts-md5": "^1.2.10",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "xlsx": "^0.17.5",
    "xss": "^1.0.11"
