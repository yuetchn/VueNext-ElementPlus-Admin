        "tinymce/plugins/template",
        "tinymce/plugins/textpattern",
        "tinymce/plugins/visualblocks",
        "tinymce/plugins/visualchars",
        "tinymce/plugins/wordcount",
        "tinymce/themes/silver/theme",
        "tinymce/tinymce",
