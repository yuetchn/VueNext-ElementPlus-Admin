    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "typescript-plugin-css-modules": "^3.4.0",
